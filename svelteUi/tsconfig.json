
 {
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "allowJs": true,
	  "checkJs": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "sourceMap": true,
	  "strict": true,
	  "types": ["svelte", "@sveltejs/kit"],
	  "rootDir": "."
	  
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules/*", ".svelte-kit/*", "public/*"]
  }
  